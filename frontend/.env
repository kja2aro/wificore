DEV=true
# API URL for auth store (goes through nginx reverse proxy)
VITE_API_URL=http://localhost/api
# API Base URL for axios (goes through nginx reverse proxy)
VITE_API_BASE_URL=http://localhost/api
# Pusher/Soketi configuration (goes through nginx reverse proxy)
VITE_PUSHER_APP_KEY=app-key
VITE_PUSHER_HOST=localhost
VITE_PUSHER_PORT=80
VITE_PUSHER_SCHEME=ws
VITE_PUSHER_APP_CLUSTER=mt1
VITE_PUSHER_AUTH_ENDPOINT=/api/broadcasting/auth
VITE_PUSHER_PATH=/app
